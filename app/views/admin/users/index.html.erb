<table>
  <tr>
    <th>First Name</th>
    <th>Last Name</th> 
    <th>Email</th>
  </tr>

  <%= paginate @users %>
  <% @users.each do |user| %>
    <tr>
      <td><%= link_to user.firstname, admin_user_path(user) %></td>
      <td><%= user.lastname %></td>
      <td><%= user.email %></td>
    </tr>
  <% end %>
</table>
